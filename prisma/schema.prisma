generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum HealthStatus {
  Healthy
  Sick
  Critical
}

model Records {
  id         String       @id @default(uuid())
  createdAt  DateTime     @default(now())
  name       String
  age        Int
  status     HealthStatus
  last_pdate DateTime     @updatedAt
}
